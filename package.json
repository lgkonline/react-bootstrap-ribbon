{
  "name": "react-bootstrap-ribbon",
  "version": "1.0.3",
  "description": "A ribbon menu for React using Bootstrap",
  "main": "index.js",
  "scripts": {
    "dev": "webpack -d --watch",
    "build": "webpack -p",
    "start": "webpack -d | webpack-dev-server --open",
    "prepare1": "babel ./src/client/lib/ReactBootstrapRibbon.jsx -o ./dist/ReactBootstrapRibbon.js",
    "prepare2": "babel ./src/client/lib/ReactBootstrapRibbonGroup.jsx -o ./dist/ReactBootstrapRibbonGroup.js",
    "prepare3": "babel ./src/client/lib/ReactBootstrapRibbonGroupItem.jsx -o ./dist/ReactBootstrapRibbonGroupItem.js",
    "prepare4": "babel ./src/client/lib/ReactBootstrapRibbonButton.jsx -o ./dist/ReactBootstrapRibbonButton.js",
    "prepare5": "copy .\\src\\client\\lib\\react-bootstrap-ribbon.css .\\dist",
    "prepare": "npm run prepare1 | npm run prepare2 | npm run prepare3 | npm run prepare4 | npm run prepare5"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lgkonline/react-bootstrap-ribbon.git"
  },
  "author": "LGK",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "copy": "^0.3.0",
    "css-loader": "^3.4.2",
    "file-loader": "^0.10.1",
    "react-bootstrap": "^0.30.8",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
